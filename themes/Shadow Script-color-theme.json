  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring"],
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#afcbff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Function Operator Member C++",
      "scope": [
        "entity.name.function.operator.member.cpp",
        "entity.name.function.operator.cpp",
        "keyword.operator.bitwise.shift.cpp"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#abdcd2"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "storage.type.function.arrow.js",
        "meta.brace.round.js",
        "meta.brace.square.js"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete",
        "keyword.control.directive",
        "constant.other.placeholder.c"
      ],
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#AFCBFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#AFCBFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#AFCBFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted",
        "string.template",
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#97e0b3"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Provided variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": [
        "support.type.property-name",
        "meta.object-literal.key",
        "variable.parameter.js"
      ],
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#AA93DD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#ffcb6b"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator.delimiter.comma.cpp",
        "keyword.control.ternary.java"
      ],
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": ""
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Python Functions Used",
      "scope": [
        "meta.function-call.generic",
        "support.type.python",
        "keyword.control",
        "constant.other.caps.python",
        "variable.language.special.self.python",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Python Variables Used",
      "scope": "source.python",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Python Logical Keywords",
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Python",
      "scope": ["support.variable.magic.python"],
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "C++ Scope Operator",
      "scope": [
        "entity.name.type.cpp",
        "entity.name.type.c",
        "storage.type.built-in.c"
      ],
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "C/C++ Includes/Definitions",
      "scope": [
        "keyword.control.directive.include.cpp",
        "keyword.control.directive.define.cpp",
        "variable.other.global.cpp",
        "punctuation.separator.pointer-access.cpp",
        "punctuation.definition.directive.cpp",
        "keyword.control.c"
      ],
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "Python Self Keyword",
      "scope": "support.type.python",
      "settings": {
        "foreground": "#AA93DD"
      }
    },
    {
      "name": "Javascript extended flow highlighting",
      "scope": [
        "keyword.control.flow.js",
        "keyword.control.conditional.js",
        "keyword.control.loop.js",
        "variable.language.this.js"
      ],
      "settings": {
        "foreground": "#AFCBFF"
      }
    },
    {
      "name": "JS/TS/React Extended Type Recognition",
      "scope": ["support.type.primitive.ts", "support.class.component.js"],
      "settings": {
        "foreground": "#AA93DD"
      }
    }
  ]
}
